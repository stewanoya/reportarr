{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System.Net.Http.HttpClient": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  },
  "HealthCheck": {
    "BaseUrl": "https://example-url.ca",
    "Endpoint": "health",
    "Interval": 60000
  },
  "Discord": {
    "Token": "your-token",
    "ApplicationId": "your-app-id",
    "PublicKey": "your-publicKey",
    "ChannelId": "channel-id",
    "AdminUserId": "tagged-user-id"
  }
}
